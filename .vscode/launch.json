  "version": "0.2.0",
  "configurations": [

    {
      "name": "Dev",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/lib/main_dev.dart"
    },
    {
      "name": "Prod",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/lib/main_prod.dart"
    },
    {
      "name": "Mock",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/lib/main_mock.dart"
    },
    {
      "name": "Release",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/lib/main_prod.dart",
      "args": [" --release"]
    },
    {
      "name": "Profile",
      "request": "launch",
      "type": "dart",
      "program": "${workspaceFolder}/lib/main_dev.dart",
      "args": ["--no-track-widget-creation"],
      "flutterMode": "profile"
    }
  ]
}